{
  "compilerOptions": {
    // No module system, concatenated file
    "module": "none",
    // Set React as the JSX factory
    "jsx": "react",
    // Target es6
    "target": "es6",
    // Include typings from built-in lib declarations
    "lib": [
      "es7",
      "dom",
      "dom.iterable"
    ],
    // Include module source maps for debugging
    "sourceMap": true,
    "moduleResolution": "node",
    "types": [
      "googlemaps"
    ]
  }
}
